{
  "manifest_version": 3,
  "name": "White Cat v3.3",
  "version": "3.3",
  "description": "Advanced Facebook fake profile reporting tool.",
  "permissions": [
    "cookies",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.facebook.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "ui.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
